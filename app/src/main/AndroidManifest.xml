<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toshiba.myapp">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivityApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Obrazek" />

        <activity android:name=".phones.Phones" />

        <activity android:name=".phones.IPhoneX" />

        <activity android:name=".phones.IPhone8" />

        <activity android:name=".phones.SamsungS8" />

    </application>

</manifest>

